<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.online" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Online</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1.2.1" />
	<platform name="android">
	  <icon src="/img/logo36.png" density="ldpi" />
	  <icon src="/img/logo48.png" density="mdpi" />
	  <icon src="/img/logo72.png" density="hdpi" />
	  <icon src="/img/logo96.png" density="xhdpi" />
    </platform>
    <access origin="*" />
	<allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
	  
</widget>
